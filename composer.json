{
    "name": "byte-artist/byte-artist",
    "description": "web server environment for byte-artist web site",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "ext-xml": "*",
        "ext-zip": "*",
        "composer-runtime-api": "^2.0",
        "friendsofsymfony/ckeditor-bundle": "^2.1",
        "geshi/geshi": "^1.0",
        "hshn/base64-encoded-file": "^3.0",
        "knpuniversity/oauth2-client-bundle": "^1.28",
        "league/oauth2-facebook": "^2.0",
        "league/oauth2-google": "^3.0",
        "sensio/framework-extra-bundle": "^5.4",
        "symfony/console": "^5.2",
        "symfony/dotenv": "^5.2",
        "symfony/flex": "^1.3.1",
        "symfony/form": "^5.2",
        "symfony/framework-bundle": "^5.2",
        "symfony/mime": "^5.2",
        "symfony/monolog-bundle": "^3.4",
        "symfony/orm-pack": "^1.0",
        "symfony/requirements-checker": "^2.0",
        "symfony/runtime": "^5.2",
        "symfony/security-bundle": "^5.2",
        "symfony/translation": "^5.2",
        "symfony/twig-bundle": "^5.2",
        "symfony/validator": "^5.2",
        "symfony/webpack-encore-bundle": "^1.6",
        "symfony/yaml": "^5.2",
        "twig/extensions": "^1.5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "mikey179/vfsstream": "^1.6",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^0.12.88",
        "phpstan/phpstan-doctrine": "^0.12.37",
        "phpstan/phpstan-phpunit": "^0.12.19",
        "phpstan/phpstan-symfony": "^0.12.33",
        "squizlabs/php_codesniffer": "^3.6",
        "symfony/browser-kit": "^5.2",
        "symfony/css-selector": "^5.2",
        "symfony/maker-bundle": "^1.12",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/profiler-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "phpstan": "phpstan analyse -c phpstan.neon src tests --level 5 --no-progress",
        "tests": "bin/simple-phpunit",
        "ci": [
            "@phpstan",
            "@tests"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "ckeditor:install --clear=drop": "symfony-cmd",
            "requirements-checker": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    }
}
